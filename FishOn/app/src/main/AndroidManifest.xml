<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mateev.fishon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fishonicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/fishonicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.AppLaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.home.HomeActivity" />
        <activity android:name=".views.history.HistoryActivity" />
    </application>

</manifest>